{
	"singleQuote": true,
	"trailingComma": "all",
	"tabWidth": 2,
	"semi": true,
	"endOfLine": "auto",
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^[./]",
		"^@/(.*)$",
		"^@assets/(.*)$",
		"^@components/(.*)$",
		"^@configs/(.*)$",
		"^@pages/(.*)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
